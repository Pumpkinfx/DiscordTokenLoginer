{
  "manifest_version": 3,
  "name": "Discord Token Manager",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://discord.com/*"]
}